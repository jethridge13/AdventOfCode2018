{
  "name": "advent-of-code-2018",
  "version": "1.0.0",
  "description": "My personal submissions for Advent of Code 2018",
  "dependencies": {
    "jest": "^23.6.0",
    "object-hash": "^1.3.1",
    "ts-md5": "^1.2.4",
    "typescript": "^3.1.6"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "devDependencies": {
    "ts-node": "^7.0.1"
  },
  "scripts": {
    "test": "tsc --noEmit -p . && jest --no-cache",
    "run": "ts-node run.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jethridge13/AdventOfCode2018.git"
  },
